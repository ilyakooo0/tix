{
  "latex-workshop.latex.outDir": "result",
  "editor.wordWrap": "on",
  "grammarly.userWords": [
    "nixpkgs",
    "typechecked",
    "typechecking"
  ],
  "latex-workshop.latex.external.build.command": "nix-build",
  "latex-workshop.latex.external.build.args": [
    "-A",
    "%DOCFILE%"
  ],
  "nixEnvSelector.suggestion": false,
  "latex-workshop.latexindent.args": [
    "-c",
    "%DIR%/",
    "%TMPFILE%",
    "-y=defaultIndent: '%INDENT%'",
    "-l"
  ]
}
